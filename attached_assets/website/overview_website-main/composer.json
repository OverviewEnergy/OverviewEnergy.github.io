{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "barrelstrength/sprout-redirects": "5.0.3",
    "barrelstrength/sprout-sitemaps": "5.0.2",
    "clubstudioltd/craft-asset-rev": "8.0.0",
    "craftcms/aws-s3": "2.2.1",
    "craftcms/ckeditor": "4.9.0",
    "craftcms/cms": "5.7.11",
    "craftcms/feed-me": "6.8.0",
    "servd/craft-asset-storage": "4.0.17",
    "topshelfcraft/wordsmith": "5.0.0",
    "trendyminds/craft-palette": "5.2.0",
    "vandres/craft-matrix-extended": "v5.0.0",
    "verbb/buttonbox": "5.0.0",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}